{
  "name": "project_movies",
  "version": "1.0.0",
  "description": "## Introduction",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start-server": "cd server && npm start",
    "start-client": "cd client && npm start",
    "test-client": "cd client && npm test",
    "test-server": "cd server && npm test",
    "start-app": "concurrently -n client,server -c cyan,magenta \"npm run start-client\" \"npm run start-server\"",
    "start-db": "pg_ctl -D /opt/homebrew/var/postgres start",
    "stop-db": "pg_ctl -D /opt/homebrew/var/postgres stop",
    "start": "npm-run-all -p start-db start-app",
    "dev": "vite --open"
  },
  "dependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.18",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "author": "Olga Dubova",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.5",
    "concurrently": "^7.6.0",
    "eslint": "^7.32.0",
    "npm-run-all": "^4.1.5"
  }
}
